        "@splidejs/splide": "^3.6.12",
        "bootstrap": "^5.1.3",
        "bootstrap-social": "^5.1.1",
        "font-awesome": "^4.7.0",
        "gsap": "^3.9.1",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1",
        "scrollmagic": "^2.0.8"
